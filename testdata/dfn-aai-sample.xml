<?xml version="1.0" encoding="UTF-8"?>
<!--
  Sample federation metadata based on DFN-AAI (German Research Network)
  https://www.aai.dfn.de/metadata/dfn-aai-idp-metadata.xml

  Contains realistic IdP entries with full metadata extensions:
  - mdui:UIInfo (DisplayName, Description, Logo, PrivacyStatementURL)
  - mdrpi:RegistrationInfo (federation registration)
  - mdattr:EntityAttributes (entity categories)
  - shibmd:Scope (domain scopes)
  - Multiple ContactPerson entries
  - Multi-language support
-->
<md:EntitiesDescriptor xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata"
                       xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
                       xmlns:mdui="urn:oasis:names:tc:SAML:metadata:ui"
                       xmlns:mdrpi="urn:oasis:names:tc:SAML:metadata:rpi"
                       xmlns:mdattr="urn:oasis:names:tc:SAML:metadata:attribute"
                       xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion"
                       xmlns:shibmd="urn:mace:shibboleth:metadata:1.0"
                       xmlns:remd="http://refeds.org/metadata"
                       ID="_dfn-aai-sample"
                       Name="https://www.aai.dfn.de"
                       validUntil="2025-12-31T23:59:59Z">

    <!-- XML Signature (simplified - real metadata has full signature) -->
    <ds:Signature>
        <ds:SignedInfo>
            <ds:CanonicalizationMethod Algorithm="http://www.w3.org/2001/10/xml-exc-c14n#"/>
            <ds:SignatureMethod Algorithm="http://www.w3.org/2001/04/xmldsig-more#rsa-sha256"/>
            <ds:Reference URI="#_dfn-aai-sample">
                <ds:Transforms>
                    <ds:Transform Algorithm="http://www.w3.org/2000/09/xmldsig#enveloped-signature"/>
                    <ds:Transform Algorithm="http://www.w3.org/2001/10/xml-exc-c14n#"/>
                </ds:Transforms>
                <ds:DigestMethod Algorithm="http://www.w3.org/2001/04/xmlenc#sha256"/>
                <ds:DigestValue>ExampleDigestValue==</ds:DigestValue>
            </ds:Reference>
        </ds:SignedInfo>
        <ds:SignatureValue>ExampleSignatureValue==</ds:SignatureValue>
        <ds:KeyInfo>
            <ds:X509Data>
                <ds:X509Certificate>MIIFederationSigningCertificate==</ds:X509Certificate>
            </ds:X509Data>
        </ds:KeyInfo>
    </ds:Signature>

    <!-- 1. Freie Universität Berlin - Full metadata example -->
    <md:EntityDescriptor entityID="https://identity.fu-berlin.de/idp-fub">
        <md:Extensions>
            <mdrpi:RegistrationInfo registrationAuthority="https://www.aai.dfn.de"
                                    registrationInstant="2010-03-15T10:00:00Z">
                <mdrpi:RegistrationPolicy xml:lang="en">https://www.aai.dfn.de/en/join/</mdrpi:RegistrationPolicy>
                <mdrpi:RegistrationPolicy xml:lang="de">https://www.aai.dfn.de/teilnahme/</mdrpi:RegistrationPolicy>
            </mdrpi:RegistrationInfo>
            <mdattr:EntityAttributes>
                <saml:Attribute Name="http://macedir.org/entity-category"
                                NameFormat="urn:oasis:names:tc:SAML:2.0:attrname-format:uri">
                    <saml:AttributeValue>http://refeds.org/category/research-and-scholarship</saml:AttributeValue>
                </saml:Attribute>
                <saml:Attribute Name="urn:oasis:names:tc:SAML:attribute:assurance-certification"
                                NameFormat="urn:oasis:names:tc:SAML:2.0:attrname-format:uri">
                    <saml:AttributeValue>https://refeds.org/sirtfi</saml:AttributeValue>
                </saml:Attribute>
            </mdattr:EntityAttributes>
        </md:Extensions>
        <md:IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
            <md:Extensions>
                <shibmd:Scope regexp="false">fu-berlin.de</shibmd:Scope>
                <mdui:UIInfo>
                    <mdui:DisplayName xml:lang="de">Freie Universität Berlin</mdui:DisplayName>
                    <mdui:DisplayName xml:lang="en">Freie Universität Berlin</mdui:DisplayName>
                    <mdui:Description xml:lang="de">Die Freie Universität Berlin zählt zu den deutschen Hochschulen, die in der Exzellenzinitiative erfolgreich waren.</mdui:Description>
                    <mdui:Description xml:lang="en">Freie Universität Berlin is one of the leading research universities in Germany.</mdui:Description>
                    <mdui:InformationURL xml:lang="de">https://www.fu-berlin.de/</mdui:InformationURL>
                    <mdui:InformationURL xml:lang="en">https://www.fu-berlin.de/en/</mdui:InformationURL>
                    <mdui:PrivacyStatementURL xml:lang="de">https://www.fu-berlin.de/datenschutz</mdui:PrivacyStatementURL>
                    <mdui:Logo height="60" width="80">https://www.fu-berlin.de/assets/img/fu-logo.png</mdui:Logo>
                    <mdui:Logo height="16" width="16">https://www.fu-berlin.de/favicon.ico</mdui:Logo>
                </mdui:UIInfo>
            </md:Extensions>
            <md:KeyDescriptor use="signing">
                <ds:KeyInfo>
                    <ds:X509Data>
                        <ds:X509Certificate></ds:X509Certificate>
                    </ds:X509Data>
                </ds:KeyInfo>
            </md:KeyDescriptor>
            <md:KeyDescriptor use="encryption">
                <ds:KeyInfo>
                    <ds:X509Data>
                        <ds:X509Certificate></ds:X509Certificate>
                    </ds:X509Data>
                </ds:KeyInfo>
            </md:KeyDescriptor>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
                                    Location="https://identity.fu-berlin.de/idp/profile/SAML2/Redirect/SSO"/>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
                                    Location="https://identity.fu-berlin.de/idp/profile/SAML2/POST/SSO"/>
        </md:IDPSSODescriptor>
        <md:Organization>
            <md:OrganizationName xml:lang="de">Freie Universität Berlin</md:OrganizationName>
            <md:OrganizationName xml:lang="en">Freie Universität Berlin</md:OrganizationName>
            <md:OrganizationDisplayName xml:lang="de">Freie Universität Berlin</md:OrganizationDisplayName>
            <md:OrganizationDisplayName xml:lang="en">Freie Universität Berlin</md:OrganizationDisplayName>
            <md:OrganizationURL xml:lang="de">https://www.fu-berlin.de/</md:OrganizationURL>
            <md:OrganizationURL xml:lang="en">https://www.fu-berlin.de/en/</md:OrganizationURL>
        </md:Organization>
        <md:ContactPerson contactType="technical">
            <md:GivenName>IT Support</md:GivenName>
            <md:EmailAddress>mailto:it-support@fu-berlin.de</md:EmailAddress>
        </md:ContactPerson>
        <md:ContactPerson contactType="support">
            <md:GivenName>User Support</md:GivenName>
            <md:EmailAddress>mailto:sso-support@fu-berlin.de</md:EmailAddress>
        </md:ContactPerson>
        <md:ContactPerson contactType="administrative">
            <md:GivenName>AAI Admin</md:GivenName>
            <md:EmailAddress>mailto:aai-admin@fu-berlin.de</md:EmailAddress>
        </md:ContactPerson>
    </md:EntityDescriptor>

    <!-- 2. Technical University of Munich - With different language preferences -->
    <md:EntityDescriptor entityID="https://tumidp.lrz.de/idp/shibboleth">
        <md:Extensions>
            <mdrpi:RegistrationInfo registrationAuthority="https://www.aai.dfn.de"
                                    registrationInstant="2008-06-20T14:30:00Z"/>
            <mdattr:EntityAttributes>
                <saml:Attribute Name="http://macedir.org/entity-category"
                                NameFormat="urn:oasis:names:tc:SAML:2.0:attrname-format:uri">
                    <saml:AttributeValue>http://refeds.org/category/research-and-scholarship</saml:AttributeValue>
                    <saml:AttributeValue>https://refeds.org/category/code-of-conduct/v2</saml:AttributeValue>
                </saml:Attribute>
            </mdattr:EntityAttributes>
        </md:Extensions>
        <md:IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
            <md:Extensions>
                <shibmd:Scope regexp="false">tum.de</shibmd:Scope>
                <shibmd:Scope regexp="false">mytum.de</shibmd:Scope>
                <shibmd:Scope regexp="true">.*\.tum\.de</shibmd:Scope>
                <mdui:UIInfo>
                    <mdui:DisplayName xml:lang="en">Technical University of Munich (TUM)</mdui:DisplayName>
                    <mdui:DisplayName xml:lang="de">Technische Universität München (TUM)</mdui:DisplayName>
                    <mdui:Description xml:lang="en">TUM is one of Europe's leading technical universities, combining top-class facilities for cutting-edge research.</mdui:Description>
                    <mdui:Description xml:lang="de">Die TUM ist eine der führenden technischen Universitäten Europas.</mdui:Description>
                    <mdui:InformationURL xml:lang="en">https://www.tum.de/en/</mdui:InformationURL>
                    <mdui:PrivacyStatementURL xml:lang="en">https://www.tum.de/en/privacy/</mdui:PrivacyStatementURL>
                    <mdui:Logo height="80" width="200">https://www.tum.de/typo3conf/ext/tum_base/Resources/Public/Images/tum-logo.svg</mdui:Logo>
                </mdui:UIInfo>
            </md:Extensions>
            <md:KeyDescriptor use="signing">
                <ds:KeyInfo>
                    <ds:X509Data>
                        <ds:X509Certificate></ds:X509Certificate>
                    </ds:X509Data>
                </ds:KeyInfo>
            </md:KeyDescriptor>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
                                    Location="https://tumidp.lrz.de/idp/profile/SAML2/Redirect/SSO"/>
        </md:IDPSSODescriptor>
        <md:Organization>
            <md:OrganizationDisplayName xml:lang="en">Technical University of Munich (TUM)</md:OrganizationDisplayName>
            <md:OrganizationDisplayName xml:lang="de">Technische Universität München (TUM)</md:OrganizationDisplayName>
        </md:Organization>
        <md:ContactPerson contactType="technical">
            <md:EmailAddress>mailto:sso-admin@lrz.de</md:EmailAddress>
        </md:ContactPerson>
    </md:EntityDescriptor>

    <!-- 3. RWTH Aachen University - Multiple scopes and certificates -->
    <md:EntityDescriptor entityID="https://login.rz.rwth-aachen.de/shibboleth">
        <md:Extensions>
            <mdrpi:RegistrationInfo registrationAuthority="https://www.aai.dfn.de"
                                    registrationInstant="2009-09-01T08:00:00Z"/>
        </md:Extensions>
        <md:IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
            <md:Extensions>
                <shibmd:Scope regexp="false">rwth-aachen.de</shibmd:Scope>
                <shibmd:Scope regexp="false">rz.rwth-aachen.de</shibmd:Scope>
                <mdui:UIInfo>
                    <mdui:DisplayName xml:lang="de">Rheinisch-Westfälische Technische Hochschule Aachen</mdui:DisplayName>
                    <mdui:DisplayName xml:lang="en">RWTH Aachen University</mdui:DisplayName>
                    <mdui:Description xml:lang="de">Die RWTH Aachen ist eine der größten und forschungsstärksten technischen Universitäten Europas.</mdui:Description>
                    <mdui:Description xml:lang="en">RWTH Aachen University is one of Europe's leading technical universities.</mdui:Description>
                    <mdui:Logo height="50" width="150">https://www.rwth-aachen.de/global/show_picture.asp?id=aaaaaaaaaakbxgd</mdui:Logo>
                    <mdui:PrivacyStatementURL xml:lang="de">https://www.rwth-aachen.de/datenschutz</mdui:PrivacyStatementURL>
                </mdui:UIInfo>
            </md:Extensions>
            <md:KeyDescriptor use="signing">
                <ds:KeyInfo>
                    <ds:X509Data>
                        <ds:X509Certificate></ds:X509Certificate>
                    </ds:X509Data>
                </ds:KeyInfo>
            </md:KeyDescriptor>
            <!-- Second signing certificate (for rotation) -->
            <md:KeyDescriptor use="signing">
                <ds:KeyInfo>
                    <ds:X509Data>
                        <ds:X509Certificate></ds:X509Certificate>
                    </ds:X509Data>
                </ds:KeyInfo>
            </md:KeyDescriptor>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
                                    Location="https://login.rz.rwth-aachen.de/idp/profile/SAML2/Redirect/SSO"/>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
                                    Location="https://login.rz.rwth-aachen.de/idp/profile/SAML2/POST/SSO"/>
        </md:IDPSSODescriptor>
        <md:Organization>
            <md:OrganizationDisplayName xml:lang="de">Rheinisch-Westfälische Technische Hochschule Aachen</md:OrganizationDisplayName>
            <md:OrganizationDisplayName xml:lang="en">RWTH Aachen University</md:OrganizationDisplayName>
        </md:Organization>
    </md:EntityDescriptor>

