        <md:Extensions>
            <mdrpi:RegistrationInfo registrationAuthority="https://www.aai.dfn.de"
                                    registrationInstant="2007-11-15T12:00:00Z"/>
            <mdattr:EntityAttributes>
                <saml:Attribute Name="http://macedir.org/entity-category"
                                NameFormat="urn:oasis:names:tc:SAML:2.0:attrname-format:uri">
                    <saml:AttributeValue>http://refeds.org/category/research-and-scholarship</saml:AttributeValue>
                </saml:Attribute>
                <saml:Attribute Name="urn:oasis:names:tc:SAML:attribute:assurance-certification"
                                NameFormat="urn:oasis:names:tc:SAML:2.0:attrname-format:uri">
                    <saml:AttributeValue>https://refeds.org/sirtfi</saml:AttributeValue>
                </saml:Attribute>
            </mdattr:EntityAttributes>
        </md:Extensions>
        <md:IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
            <md:Extensions>
                <shibmd:Scope regexp="false">mpg.de</shibmd:Scope>
                <shibmd:Scope regexp="true">.*\.mpg\.de</shibmd:Scope>
                <mdui:UIInfo>
                    <mdui:DisplayName xml:lang="en">Max Planck Society</mdui:DisplayName>
                    <mdui:DisplayName xml:lang="de">Max-Planck-Gesellschaft</mdui:DisplayName>
                    <mdui:Description xml:lang="en">The Max Planck Society is Germany's most successful research organization.</mdui:Description>
                    <mdui:Description xml:lang="de">Die Max-Planck-Gesellschaft ist Deutschlands erfolgreichste Forschungsorganisation.</mdui:Description>
                    <mdui:Logo height="60" width="200">https://www.mpg.de/assets/images/mpg-logo.svg</mdui:Logo>
                    <mdui:InformationURL xml:lang="en">https://www.mpg.de/en</mdui:InformationURL>
                    <mdui:PrivacyStatementURL xml:lang="en">https://www.mpg.de/privacy</mdui:PrivacyStatementURL>
                </mdui:UIInfo>
            </md:Extensions>
            <md:KeyDescriptor use="signing">
                <ds:KeyInfo>
                    <ds:X509Data>
                        <ds:X509Certificate></ds:X509Certificate>
                    </ds:X509Data>
                </ds:KeyInfo>
            </md:KeyDescriptor>
            <md:SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
                                    Location="https://idp.mpg.de/idp/profile/SAML2/Redirect/SSO"/>
        </md:IDPSSODescriptor>
        <md:Organization>
            <md:OrganizationDisplayName xml:lang="en">Max Planck Society</md:OrganizationDisplayName>
            <md:OrganizationDisplayName xml:lang="de">Max-Planck-Gesellschaft</md:OrganizationDisplayName>
        </md:Organization>
        <md:ContactPerson contactType="technical">
            <md:GivenName>MPG IT Security</md:GivenName>
            <md:EmailAddress>mailto:sirtfi@mpg.de</md:EmailAddress>
        </md:ContactPerson>
    </md:EntityDescriptor>

