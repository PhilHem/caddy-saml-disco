                    <X509Data>
                        <X509Certificate>MIICpDCCAYwCCQDU+pQ4P2dP3jANBgkqhkiG9w0BAQsFADAUMRIwEAYDVQQDDAls</X509Certificate>
                    </X509Data>
                </KeyInfo>
            </KeyDescriptor>
            <SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
                                 Location="https://idp2.example.com/saml/sso"/>
        </IDPSSODescriptor>
        <Organization>
            <OrganizationDisplayName xml:lang="en">Second IdP</OrganizationDisplayName>
        </Organization>
    </EntityDescriptor>

    <!-- IdP 3 -->
    <EntityDescriptor entityID="https://idp3.example.com/saml">
        <IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
            <KeyDescriptor use="signing">
                <KeyInfo xmlns="http://www.w3.org/2000/09/xmldsig#">
                    <X509Data>
                        <X509Certificate>MIICpDCCAYwCCQDU+pQ4P2dP3jANBgkqhkiG9w0BAQsFADAUMRIwEAYDVQQDDAls</X509Certificate>
                    </X509Data>
                </KeyInfo>
            </KeyDescriptor>
            <SingleSignOnService Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
                                 Location="https://idp3.example.com/saml/sso"/>
        </IDPSSODescriptor>
        <Organization>
            <OrganizationDisplayName xml:lang="en">Third IdP</OrganizationDisplayName>
        </Organization>
    </EntityDescriptor>
</EntitiesDescriptor>
